<?xml version="1.0" encoding="utf-8"?><!-- @drawable/layout_button_login_round_background.xml -->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false">
        <shape>
            <corners android:radius="50dp" />
            <solid android:color="@color/ic_login_background" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape>
            <corners android:radius="50dp" />
            <solid android:color="@color/ic_login_background_pressed" />
        </shape>
    </item>
</selector>
